<%= hidden_field_tag :classroom_id, params[:classroom_id] %>
<%= f.input :classroom_id, :as => "hidden"%>
<div class="row-fluid">
  <div class="span6"><%= f.input :name, input_html: { class: 'span12' } %></div>
</div>
<div class="row-fluid">
  <div class="span4">
  <%= f.input :birth_date, start_year: Date.today.year - 63,
                              end_year: Date.today.year + 37, input_html: { class: 'span4' } %>
  </div>
</div>
<div class="row-fluid">
  <div class="span6"><%= f.input :email, input_html: { class: 'span12' } %></div>
</div>

<div class="row-fluid">
  <div class="span6"><%= f.input :document, input_html: { class: 'span12' } %></div>
</div>

<div class="row-fluid">
  <div class="span3"><%= f.input :phone, input_html: { class: 'span8 phone' } %></div>
  <div class="span3"><%= f.input :cell_phone, input_html: { class: 'span8 phone' } %></div>
</div>
