<div class="row-fluid">
  <div class="span6"><%= f.input :name, input_html: { class: 'span12' } %></div>
</div>
<div class="row-fluid">
  <div class="span4">
  <%= f.input :birth_date, start_year: Date.today.year - 63,
                              end_year: Date.today.year + 37, input_html: { class: 'span4' } %>
  </div>
</div>
<div class="row-fluid">
  <div class="span6"><%= f.input :mother, input_html: { class: 'span12' } %></div>
  <div class="span6"><%= f.input :father, input_html: { class: 'span12' } %></div>

</div>

<div class="row-fluid">
  <div class="span6"><%= f.input :email, input_html: { class: 'span12' } %></div>
</div>

<div class="row-fluid">
  <div class="span6"><%= f.input :document, input_html: { class: 'span12' } %></div>
</div>

<div class="row-fluid">
  <div class="span3"><%= f.input :phone, input_html: { class: 'span8 phone' } %></div>
  <div class="span3"><%= f.input :cell_phone, input_html: { class: 'span8 phone' } %></div>
</div>
<div class="row-fluid">
    <%= f.input :obs, input_html: { rows: '10', cols: '100' } %>
</div>

<div class="row-fluid">
  <div class="span6"><%= f.input :block_schedule_different %></div>
</div>
