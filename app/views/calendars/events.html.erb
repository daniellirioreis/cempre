<% @group_active.each do |g| %>
  <i class="icon-calendar"></i> <%= g.classroom%> - <%= g.classroom.calendar%><br><br>
  <% contador = 0 %>
  <table class="table table-striped table-hover table-bordered">
      <% g.classroom.lessons.each do |l| %>
        <% if contador == 0 %>
          <tr>
          <%end%>
          <%  contador = contador + 1 %>
          <td>
            <%= l %><br>
            <% l.schedules.each do |s| %>
              <%= s.description %>
            <%end%>
          </td>

          <% if contador == 4 %>
            <%contador = 0%>
          </tr>
        <%end%>
      <% end %>
    </table>
<% end %>