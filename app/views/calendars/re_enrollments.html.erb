<strong>Quantidades rematricula: </strong><%= @groups_re_enrollments.count %><br><br>
<table class="table table-striped table-hover table-bordered">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Status</th>
      <th>Turma</th>
    </tr>
  </thead>
  <tbody>
    <%@groups_re_enrollments.each do |g| %>
     <tr>
        <td>
          <%= g %>
        </td>
        <td>
          <%= g.status_humanize %>
        </td>
        <td>
          <%= g.classroom %>
        </td>
     </tr>
     <% end %>
  </tbody>
</table>