<script src='http://www.google.com/jsapi'></script>




<div class="tabbable">
  <ul class="nav nav-tabs">
    <li class="active"> <%= link_to "Graficos", "#all_charts", :"data-toggle" => :tab %></li>
    <li> <%= link_to "Histórico de Movimentação", "#histories", :"data-toggle" => :tab %></li>
    <li> <%= link_to "Faltas", "#faults", :"data-toggle" => :tab %></li>
  </ul>
</div>

<div class="tab-content">
  <div class="tab-pane fade active in" id="all_charts">
		<div id='chart'></div>
		<div id='chart1'></div>
		<%= render_chart @chart, 'chart' %>
		<%= render_chart @chart1, 'chart1' %>	
	</div>
	
  <div class="tab-pane" id="histories">
		<%= render 'info_students' %>	
	</div>  
	
	<div class="tab-pane" id="faults">
		<%= render 'faults' %>	
	</div>  
  
</div>





