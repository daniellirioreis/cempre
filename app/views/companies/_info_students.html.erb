<% @company.calendars.sorted.each do |c| %>
  <h4><%=c%></h4><br>
	<% total_groups = 0 %>
	<% StatusGroup.to_a.each do |s| %>
		<% groups = c.groups.status(s.last.to_i)%>
		<% count_groups = groups.count %>
		<% total_groups = total_groups + count_groups %>
		<h5><%= s.first %>(<%= count_groups %>)</h5>
  <% end %>
	<h5> Total: <%= total_groups %><h5>
<%end%>