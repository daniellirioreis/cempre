<h1>Listing calendar_days</h1>

<table>
  <thead>
    <tr>
      <th>Day</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_days.each do |calendar_day| %>
      <tr>
        <td><%= calendar_day.day %></td>
        <td><%= link_to 'Show', calendar_day %></td>
        <td><%= link_to 'Edit', edit_calendar_day_path(calendar_day) %></td>
        <td><%= link_to 'Destroy', calendar_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar day', new_calendar_day_path %>
